/*dede58.com  做最好的织梦模板*/
html {
	overflow:hidden;
	overflow-x:hidden;
	overflow-y:auto;
}
body, ul, ul li, ul ol, h1, h2, h3, h4, h5, h6, img, a, p, input, em, i {
	margin: 0px;
	list-style: none;
	font-family: 微软雅黑;
	border: 0px;
	font-size: 14px;
	font-style: normal;
	font-weight: normal;
	font-style:normal;
	padding-bottom: 0px;
	padding-left: 0px;
	padding-right: 0px;
	/* [disabled]padding-top: 0px;
*/
}  /*格式化初始数据*/
input, textarea {
	outline:medium;
}
